<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: linear-gradient(to bottom, #383a3d, grey);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bid.bidalot.controllers.AdminViewController">
   <children>
      <Label layoutX="111.0" layoutY="14.0" text="Administrator Menu" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="54.0" layoutY="75.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="25.0" prefWidth="115.0" style="-fx-background-color: linear-gradient(to bottom, #dbdbdb, #7d7d7d); -fx-background-insets: 0; -fx-border-color: black; -fx-border-width: 0.5;" text="Save" />
      <Button layoutX="225.0" layoutY="75.0" mnemonicParsing="false" onAction="#loadButton" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: linear-gradient(to bottom, #dbdbdb, #7d7d7d); -fx-background-insets: 0; -fx-border-color: black; -fx-border-width: 0.5;" text="Load" />
      <Button fx:id="clearAll" layoutX="225.0" layoutY="126.0" mnemonicParsing="false" onAction="#clearAll" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: linear-gradient(to bottom, #dbdbdb, #7d7d7d); -fx-background-insets: 0; -fx-border-color: black; -fx-border-width: 0.5;" text="Clear All" />
      <Button fx:id="closeButton" layoutX="54.0" layoutY="126.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="25.0" prefWidth="115.0" style="-fx-background-color: linear-gradient(to bottom, #dbdbdb, #7d7d7d); -fx-background-insets: 0; -fx-border-color: black; -fx-border-width: 0.5;" text="Return" />
   </children>
</AnchorPane>
